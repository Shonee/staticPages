{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      { "src": ".css", "use": "@vercel/static-build" }, 
      { "src": "*.js", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
